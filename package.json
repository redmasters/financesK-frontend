{
  "name": "finances-k-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:staging": "ng serve --configuration=staging",
    "build": "ng build",
    "build:dev": "ng build --configuration=development",
    "build:staging": "ng build --configuration=staging",
    "build:prod": "ng build --configuration=production",
    "build:vercel": "ng build --configuration=vercel",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "docker:build": "docker build -t financesK-frontend .",
    "docker:build:prod": "docker build -t financesK-frontend:prod -f Dockerfile.prod .",
    "docker:run": "docker run -p 4200:80 financesK-frontend",
    "deploy:staging": "npm run build:staging && echo 'Deploy staging build to your staging server'",
    "deploy:prod": "npm run build:prod && echo 'Deploy production build to your production server'",
    "deploy:vercel": "npm run build:vercel && vercel --prod",
    "preview:vercel": "npm run build:vercel && vercel",
    "vercel:install": "npm ci --legacy-peer-deps",
    "analyze": "npm run build:vercel -- --stats-json && npx webpack-bundle-analyzer dist/finances-k-front/stats.json"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "bulma": "^1.0.4",
    "chart.js": "^4.5.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.0",
    "@types/chart.js": "^2.9.41",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
